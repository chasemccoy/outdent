    "@types/node": "^8.0.44",
    "@types/rimraf": "^2.0.2",
    "@types/which": "^1.0.28",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.4.0",
